.altsalt {
	padding: 10px 5px 0 5px;
	font-size: 10px;
}

.timeline-utilities-window Button {
	height: 20px;
}

.timeline-utilities-window IntegerField .unity-integer-field__input {
	-unity-text-align: middle-center;
}

.section-container {
	margin-bottom: 10px;
}

.section {
	flex-direction: column;
	flex-wrap: wrap;
	align-items: center;
	justify-content: center;
	padding-top: 10px;
}

.section > * {
	flex-direction: row;
	flex-wrap: wrap;
	align-items: flex-start;
	justify-content: center;
	max-width: 275px;
}

.section___title {
	font-size: 12px;
	margin-bottom: 5px;
}

.section___content {
	margin-bottom: 10px;
}

.selection {
	margin-bottom: 5px;
}

.selection .select-btn {
	flex-grow: 1;
	flex-shrink: 1;
	width: 57.5px;
}

.selection .tilde {
	margin: 0 10px 0 10px;
	width: 25px;
	font-size: 20px;
	-unity-text-align: middle-center;
}

.selection .select-count {
	margin: 0 10px 0 10px;
	width: 25px;
}

.selection .unity-integer-field__input {
	padding-top: 2px;
	-unity-text-align: middle-center;
}

.manipulate .manipulate-toggle-transpose {
	width: 200px;
	justify-content: flex-start;
	-unity-text-align: middle-right;
}

.unity-toggle__input {
	flex-direction: row;
	width: 200px;
	justify-content: flex-start;
	-unity-text-align: middle-right;
}

.col-1 {
	flex-grow: 1;
	width: 70px;
}

.col-2 {
	flex-grow: 1;
	margin-right: 5px;
	width: 35px;
}

.col-3 {
	flex-grow: 1;
	margin-left: 5px;
	width: 77.5px;
}

.col-full {
	flex-grow: 1;
	width: 275px;	
	margin-bottom: 5px;
}

.col-1.placeholder {
	flex-grow: 0;
	flex-shrink: 1;
}

.col-2.placeholder {
	flex-grow: 0;
	flex-shrink: 1;
}

.persistent-float-field {
	background-image: none;
}